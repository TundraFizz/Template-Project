  "description": "Template Project",
  "author": "Tundra Fizz",
  "private": true,
  "scripts": {
    "api_local": "set mode=local&& nodemon --watch backend -e ts --exec node -r ts-node/register backend/server.ts",
    "vue_local": "set local=true&& vue-cli-service serve --port 8001 --mode development --open src/main.ts",
    "api_dev": "tsc; mode=dev node backend/out/server.js",
    "vue_dev": "NODE_ENV=dev vue-cli-service serve --mode production --open src/main.ts",
    "api_prod": "set mode=prod&& node backend/server.js",
    "vue_prod_old_2": "NODE_ENV=prod vue-cli-service serve --port 8001 --mode production --open src/main.ts",
    "vue_prod": "vue-cli-service serve --mode production",
    "vue_prod_orig": "NODE_ENV=prod vue-cli-service serve --port 8001 --mode production",
    "build_frontend": "vue-cli-service build --mode production",
    "build_frontend_2": "vue-cli-service build --modern --mode production; purgecss --css ./dist/bootstrap-4.5.3.min.css --content ./dist/**/*.js --output ./dist/bootstrap-4.5.3.min.css",
    "build_frontend_3": "NODE_ENV=prod vue-cli-service build --mode production",
    "build_backend": "tsc",
    "serve_frontend": "node server.js",
    "serve_frontend_2": "serve -s dist --no-clipboard --listen 8001",
    "build_frontend_test": "vue-cli-service build --mode production",
    "deploy_frontend": "TODO"
  },
